{
  "name": "sfcc-meta-object-manager",
  "displayName": "SFCC Meta Object Manager",
  "description": "A VS Code extension for managing Salesforce Commerce Cloud (SFCC) meta-system objects, updating custom attributes, and managing group definitions.",
  "version": "0.1.0",
  "publisher": "YourPublisherName",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/sfcc-meta-object-manager.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onCommand:sfcc-meta-object-manager.uploadMetaFile",
    "onCommand:sfcc-meta-object-manager.updateWholeTypeExtension",
    "onCommand:sfcc-meta-object-manager.updateDetailedTypeExtension"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "lint": "eslint . --ext ts"
  },
  "dependencies": {
    "vscode": "^1.1.37",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/vscode": "^1.60.0",
    "@types/xml2js": "^0.4.14",
    "eslint": "^7.32.0",
    "typescript": "^4.4.3"
  },
  "contributes": {
    "commands": [
      {
        "command": "sfcc-meta-object-manager.uploadMetaFile",
        "title": "Upload SFCC Meta File"
      },
      {
        "command": "sfcc-meta-object-manager.updateWholeTypeExtension",
        "title": "Update Whole Type-Extension"
      },
      {
        "command": "sfcc-meta-object-manager.updateDetailedTypeExtension",
        "title": "Update Detailed Type-Extension"
      }
    ]
  }
}
